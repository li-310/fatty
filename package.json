  "description": "业务组件库",
  "scripts": {
    "dev": "vite",
    "build": "npm run clean && npm run build:2 && npm run build:3",
    "type-check": "vue-tsc --noEmit",
    "switch:2": "npx vue-demi-switch 2 vue2",
    "switch:3": "npx vue-demi-switch 3 vue3",
    "build:3": "npm run switch:3 && rollup -c",
    "build:2": "npm run switch:2 && rollup -c",
    "clean": "rimraf ./dist",
    "postinstall": "node ./scripts/postinstall.mjs"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/li-310/fatty.git"
  },
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "files": [
    "dist"
  ],
