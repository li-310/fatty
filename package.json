{
  "name": "@leo-mm/fatty",
  "version": "1.0.18",
  "description": "业务组件库",
  "scripts": {
    "dev:2": "npm run switch:2 && vite",
    "dev:3": "npm run switch:3 && vite",
    "prebuild": "npm run clean",
    "build": "npm run build:2 && npm run build:3",
    "build:2": "npm run switch:2 && vite build",
    "build:3": "npm run switch:3 && vite build",
    "type-check": "vue-tsc --noEmit",
    "switch:2": "npx vue-demi-switch 2 vue2",
    "switch:3": "npx vue-demi-switch 3 vue3",
    "clean": "rimraf ./dist",
    "preview": "vite preview --port 8080",
    "postinstall": "node ./scripts/postinstall.mjs"
  },
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/li-310/fatty.git"
  },
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/li-310/fatty/issues"
  },
  "homepage": "https://github.com/li-310/fatty#readme",
  "peerDependencies": {
    "@vue/composition-api": "^1.1.4",
    "vue": "^2.0.0 || >=3.0.0",
   "element-ui": "^2",
   "element-plus": "^3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.3",
    "element-plus": "^2.9.11",
    "element-ui": "^2.15.14",
    "rimraf": "^3.0.2",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.7.0",
    "unplugin-vue2-script-setup": "0.11.3",
    "vite": "^6.3.5",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-vue2": "^2.0.2",
    "vue": "2.6.14",
    "vue-demi": "^0.13.7",
    "vue-template-compiler": "2.6.14",
    "vue2": "npm:vue@2.6.14",
    "vue3": "npm:vue@^3.2.36"
  },
  "dependencies": {
    "vue-demi": "^0.13.7"
  },
  "files": ["dist", "scripts", "package.json"],
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    },
    "element-ui": {
      "optional": true
    },
    "element-plus": {
      "optional": true
    }
  }
}
